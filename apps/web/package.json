  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "prebuild": "node scripts/setup-convex.js",
    "build": "npm run generate:swagger && next build",
    "start": "next start",
    "generate:swagger": "node scripts/generate-swagger-static.js",
    "lint": "next lint",
    "test": "vitest run tests --config=vitest.config.ts",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:verbose": "vitest --reporter=verbose",
    "test:verbose:watch": "vitest --watch --reporter=verbose",
    "test:detailed": "vitest --reporter=verbose --log-level=info",
    "test:debug": "vitest --reporter=verbose --log-level=debug",
    "test:api": "vitest tests/api/",
    "test:api:verbose": "vitest tests/api/ --reporter=verbose",
    "test:api:detailed": "vitest tests/api/ --reporter=verbose --log-level=info",
    "test:vitest": "vitest run --config=vitest.config.ts",
    "test:vitest:watch": "vitest watch --config=vitest.config.ts",
    "test:unit": "vitest run --config=vitest.config.ts --reporter=verbose",
    "test:coverage:vitest": "vitest run --config=vitest.config.ts --coverage",
    "test:integration": "vitest run tests/integration --config=vitest.config.ts",
    "test:jest": "jest",
    "test:jest:watch": "jest --watch",
    "test:all": "bun test:vitest && bun test:jest",
    "test:all:watch": "concurrently \"bun test:vitest:watch\" \"bun test:jest:watch\" --names \"VITE,JEST\"",
    "test:setup": "bash scripts/test-setup.sh",
    "seed:admin": "tsx scripts/seed-admin-data.ts",
    "seed:careers": "bun run scripts/seed-careers.ts",
    "create:admin": "tsx scripts/create-admin-user.ts",
    "migrate:blog": "tsx scripts/migrate-blog-posts.ts",
    "vercel:sync-env": "bash scripts/sync-env-to-vercel.sh",
    "vercel:env": "vercel env ls",
    "vercel:login": "vercel login"
